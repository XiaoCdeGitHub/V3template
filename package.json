{
  "name": "V3TEMPLATE",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint:eslint": "eslint --cache --max-warnings 0 {src,mock}/**/*.{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --write **/*.{js,json,tsx,css,less,scss,vue,html,md}",
    "lint:stylelint": "stylelint --ignore-path ./config/stylelint/.stylelintignore --config ./config/stylelint/.stylelint.config.js {src}**/*.{css,less,scss,vue,html} --fix",
    "lint:lint-staged": "lint-staged -c ./config/lint-staged/lint-staged.config.js",
    "commit": "git add . && pnpm lint:lint-staged && czg --config=./config/czg/cz.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.5.0",
    "element-plus": "^2.3.12",
    "pinia": "^2.1.6",
    "unplugin-icons": "^0.17.0",
    "vite-plugin-inspect": "^0.7.38",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.4.6",
    "@iconify-json/ep": "^1.1.12",
    "@iconify/json": "^2.2.116",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.15",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/tsconfig": "^0.4.0",
    "czg": "^1.7.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.8",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.2.1",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}
